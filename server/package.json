    "@types/mongoose-unique-validator": "^1.0.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.9.0",
    "command-line-args": "^5.1.1",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2"

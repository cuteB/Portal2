    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.1.4",
    "jade": "^1.9.2",
    "mongoose": "^5.12.14",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "webpack": "4.44.2",
    "webpack-cli": "^4.7.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/helmet": "^4.0.0",
